#!/usr/bin/env node

var program =  require('commander');

program
  .version("0.0.1")
  .command('appdev <command> [args]', 'Application Developer interface')
  .command('ops <command> [args]', 'Operation Manager interface')
  .command('stackdev <command> [args]', 'Stack Developer interfacce')
  .parse(process.argv);

